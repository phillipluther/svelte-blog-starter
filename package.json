{
  "name": "svelte-blog-starter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "prepare": "npm run smui-theme compile static/smui.css src/lib/styles/theme",
    "smui-theme": "smui-theme compile static/smui.css -i src/theme"
  },
  "devDependencies": {
    "@phillipluther/eslint-config": "^0.1.3",
    "@phillipluther/prettier-config": "^0.1.2",
    "@smui/drawer": "^7.0.0-beta.0",
    "@sveltejs/adapter-auto": "^1.0.0",
    "@sveltejs/adapter-static": "^1.0.6",
    "@sveltejs/kit": "^1.0.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "feed": "^4.2.2",
    "mdsvex": "^0.10.6",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-slug": "^5.1.0",
    "smui-theme": "^7.0.0-beta.0",
    "svelte": "^3.54.0",
    "vite": "^4.0.0",
    "vitest": "^0.25.3"
  },
  "type": "module",
  "dependencies": {
    "title-case": "^3.0.3"
  },
  "prettier": "@phillipluther/prettier-config"
}
